# PostgreSQL Database Configuration
PG_PASS=your_secure_postgres_password_here
PG_USER=authentik
PG_DB=authentik

# Authentik Secret Key (generate with: openssl rand -base64 60)
AUTHENTIK_SECRET_KEY=your_secret_key_here_at_least_50_chars_long_random_string

# Authentik Configuration
AUTHENTIK_LOG_LEVEL=info
AUTHENTIK_PORT_HTTP=9000
AUTHENTIK_PORT_HTTPS=9443

# Email Configuration (optional)
# AUTHENTIK_EMAIL__HOST=smtp.gmail.com
# AUTHENTIK_EMAIL__PORT=587
# AUTHENTIK_EMAIL__USERNAME=your-email@gmail.com
# AUTHENTIK_EMAIL__PASSWORD=your-app-password
# AUTHENTIK_EMAIL__USE_TLS=true
# AUTHENTIK_EMAIL__FROM=noreply@yourdomain.com
